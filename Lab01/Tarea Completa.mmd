classDiagram
    class Estudiante {
        -id: int
        -nombre: string
        -email: string
        -carrera: string
        +registrar(): void
        +editarPerfil(): void
        +consultarCursosDisponibles(): List<Curso>
        +consultarCursosMatriculados(): List<Curso>
    }
 

    class Curso {
        -codigo: string
        -nombre: string
        -creditos: int
        +matricular(): void
        +retirar(): void
    }


    class Periodo {
        -anio: int
        -nombre: string
        -activo: boolean
       +asignarCurso(): void
       +consultarCursos(): List<Curso>
    }
 
  class Profesor {
        -idProfesor: int
        -nombre: string
        -correo: string
        +asignarCurso(): void
        +consultarCursosAsignados(): List<PeriodoCurso>
    }

  class Matricula {
        -idMatricula: int
        -idEstudiante: int
        -idGrupo: int
        -fecha: date
    }

  class Grupo {
        -id_grupo: int
        -idCurso: string
        -idProfesor: int?
        -idPeriodo: int
        -capacidad: int
        -numero_grupo: int
        -horario: string
        +agregarEstudiante(): void
        +consultarEstudiantes(): List<Estudiante>
    }

